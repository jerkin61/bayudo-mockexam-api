web: vendor/bin/heroku-php-nginx -C nginx.conf public/
migrate: php artisan migrate
migrate-path: php artisan migrate --packages/jerquin-bayudo/chatbot-api/database/migrations
seed: php artisan db:seed
seed-refresh: php artisan migrate:fresh --seed
route: php artisan route:list
storage: php artisan storage:link
passport: php artisan passport:install
passport-keys: php artisan passport:keys
passport-personal: php artisan passport:client --personal
version: php artisan --version
cron: php artisan schedule:check
optimize: php artisan optimize
chatbot-user: php artisan chatbot-jerquin-bayudo:install